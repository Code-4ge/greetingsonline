{
    "version": 2,
    "builds": [{
        "src": "index.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
      "src": "build.sh",
      "use": "@vercel/static-build"
    }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },    

        {
            "src": "/(.*)",
            "dest": "index.py"
        }
    ]
}